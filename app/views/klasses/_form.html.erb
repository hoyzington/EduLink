<%= render 'layouts/errors', obj: @klass %>
<%= form_with model: @klass, local: true, class: "" do |f| %>
  <%= f.hidden_field :teacher_id %>
  <%= f.label :name, "Class Name" %>
  <%= f.text_field :name %>
  <%= f.label :period, "Period" %>
  <%= f.text_field :period %>
  <%= f.submit "Submit" %>
<% end %>
