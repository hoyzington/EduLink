<!DOCTYPE html>
<html>
  <head>
    <title>EduLink</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render 'layouts/navbar' %>
    <h3><%= render 'layouts/flash_msgs' if flash.keys %></h3>
    <br>
    <%= yield %>
  </body>
</html>
