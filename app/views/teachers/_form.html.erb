<%= form_with model: @teacher, class: "" do |f| %>
  <%= f.label :id_number, "Teacher ID" %>
  <%= f.number_field :id_number %>
  <%= f.label :first_name, "First Name" %>
  <%= f.text_field :first_name %>
  <%= f.label :last_name, "Last Name" %>
  <%= f.text_field :last_name %>
  <%= f.label :dept, "Department" %>
  <%= f.text_field :dept %>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit "Submit" %>
<% end %>
