<h1>William McKinley High School's EduLink</h1>
<br>
<h1>Log In</h1>
<br>
<%= form_with scope: :session, url: login_path do |f| %>
  <h3><%= f.label :email %>
  <%= f.email_field :email %></h3>
  <h3><%= f.label :password %>
  <%= f.password_field :password %></h3>
  <h3><%= f.submit "Log In" %></h3>
<% end %>
